<% include partials/header %>

  <h1>Genetic Javascript</h1>

  <div class="container">

    <div>
      <dl class="row">
        <dt class="col-sm-3">Goal String: </dt>
        <dd class="col-sm-9">"
          <%= goalString %>"</dd>

        <dt class="col-sm-3">Population Size: </dt>
        <dd class="col-sm-9">
          <%= populationSize %>
        </dd>

        <dt class="col-sm-3">Mutation Rate(%): </dt>
        <dd class="col-sm-9">
          <%= mutationRate %>
        </dd>

        <dt class="col-sm-3">CrossoverRate(%): </dt>
        <dd class="col-sm-9">
          <%= crossoverRate %>
        </dd>

        <dt class="col-sm-3">Generation: </dt>
        <dd class="col-sm-9">undefined</dd>

        <dt class="col-sm-3">Most Fit Strand: </dt>
        <dd class="col-sm-9">"undefined"</dd>

        <dt class="col-sm-3">Least Fit Strand: </dt>
        <dd class="col-sm-9">"undefined"</dd>
      </dl>
    </div>

    <div class="progress">
      <div class="progress-bar progress-bar-striped progress-bar-animated bg-success" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width:60%">
        Most Fit
      </div>
    </div>

    <div class="progress">
      <div class="progress-bar progress-bar-striped progress-bar-animated bg-danger" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width:30%">
        Least Fit
      </div>
    </div>

  </div>

  <% include partials/footer %>
